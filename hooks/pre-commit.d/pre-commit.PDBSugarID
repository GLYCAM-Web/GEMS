#!/bin/bash

#runs the script with a functional argument
PDBSugarID "gmml/example/pdb/1NXC.pdb" "test_output" "keep"

#If the output file was created, success
if [ -f "test_output" ] ; then
  rm "test_output"
  rm "test_output_sugar-details"
#If the file was not found, fail
else
        echo "$test_output not found."
    exit 1
fi


